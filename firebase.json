{
    "hosting": {
        "site": "gca-hackathon",
        "public": "dist",
        "rewrites": [
            {
                "source": "/api/**",
                "run": {
                    "serviceId": "gca-be",
                    "region": "us-central1"
                }
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}